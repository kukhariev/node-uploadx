{
  "name": "node-uploadx-src",
  "version": "4.5.0",
  "description": "Node.js resumable upload middleware",
  "keywords": [
    "resumable",
    "upload",
    "express",
    "uploadx",
    "middleware",
    "s3",
    "gcloud"
  ],
  "author": {
    "name": "Oleg Kukhariev",
    "url": "https://github.com/kukhariev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/node-uploadx.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.22.0",
    "npm": "use yarn instead",
    "yarn": ">=1.22.15"
  },
  "scripts": {
    "start": "yarn --cwd examples run basic",
    "build": "tsc --build packages",
    "build:watch": "tsc --build --watch packages",
    "clean": "tsc --build packages --clean && rimraf packages/*/lib upload files dist",
    "lint": "eslint . --fix --ext .js,.ts",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{json,md}\"",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/node": "14.17.34",
    "@types/rimraf": "3.0.2",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "express": "4.17.1",
    "husky": "7.0.4",
    "jest": "27.4.0",
    "lint-staged": "12.1.2",
    "node-mocks-http": "1.11.0",
    "oao": "2.0.2",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.2"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "examples"
  ],
  "lint-staged": {
    "*.{md,json}": "prettier --write",
    "{packages,examples,test}/**/*.ts": "eslint --fix"
  }
}
