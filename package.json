{
  "name": "node-uploadx",
  "version": "2.0.2",
  "description": "Node.js middleware for handling resumable uploads",
  "keywords": [
    "upload",
    "chunk",
    "express",
    "middleware"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/node-uploadx.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "configstore": "^5.0.0",
    "debug": "^4.1.1"
  },
  "scripts": {
    "start": "tsnd test/server.ts",
    "dev": "tsnd --respawn --transpileOnly test/server.ts",
    "prettier": "prettier --write \"**/*.{json,md,ts}\"",
    "build": "rimraf dist && tsc",
    "postbuild": "tsnd scripts/post-build.ts",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,js}\" --fix",
    "demo:node": "npm run build && node examples/node",
    "demo:express": "tsnd examples/express",
    "demo:fastify": "tsnd  examples/fastify/"
  },
  "devDependencies": {
    "@types/bytes": "^3.0.0",
    "@types/chai": "^4.2.0",
    "@types/configstore": "^4.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/rimraf": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "express": "^4.17.1",
    "fastify": "^2.8.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "mocha": "^6.2.0",
    "node-mocks-http": "^1.7.6",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
