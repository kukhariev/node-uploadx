    "demo:node": "node examples/node/index.js",
    "demo:express": "node examples/express/index.js",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@types/bytes": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/configstore": "^4.0.0",
