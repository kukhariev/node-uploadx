{
  "name": "node-uploadx-src",
  "version": "4.2.2",
  "description": "Node.js resumable upload middleware",
  "keywords": [
    "resumable",
    "upload",
    "express",
    "uploadx",
    "middleware",
    "s3",
    "gcloud"
  ],
  "author": {
    "name": "Oleg Kukhariev",
    "url": "https://github.com/kukhariev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/node-uploadx.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.10.0",
    "npm": "use yarn instead",
    "yarn": ">= 1.22.0"
  },
  "scripts": {
    "build": "tsc --build packages",
    "build:watch": "tsc --build --watch packages",
    "clean": "tsc --build packages --clean && rimraf packages/*/lib upload files dist",
    "lint": "eslint . --fix --ext .js,.ts",
    "prepare": "yarn run build",
    "prettier": "prettier --write \"**/*.{json,md}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/node": "10.17.54",
    "@types/rimraf": "3.0.0",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "express": "4.17.1",
    "husky": "5.1.2",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "node-mocks-http": "1.10.1",
    "oao": "2.0.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.2",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.2"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "examples"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,json}": "prettier --write",
    "{packages,examples,test}/**/*.ts": "eslint --fix"
  }
}
