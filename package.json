{
  "name": "node-uploadx",
  "version": "4.0.0",
  "description": "Node.js resumable upload middleware",
  "keywords": [
    "resumable",
    "upload",
    "express",
    "fastify",
    "middleware",
    "s3",
    "gcloud"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/node-uploadx.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.10.0"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "aws-sdk": "^2.593.0",
    "bytes": "^3.1.0",
    "debug": "^4.1.1",
    "google-auth-library": "^5.7.0",
    "multiparty": "^4.2.1",
    "node-fetch": "^2.6.0"
  },
  "scripts": {
    "start": "tsnd test/server.ts",
    "dev": "tsnd --respawn --transpileOnly test/server.ts --log",
    "prettier": "prettier --write \"**/*.{json,md}\"",
    "build": "rimraf dist && tsc",
    "postbuild": "tsnd scripts/post-build.ts",
    "cleanup": "rimraf upload files dist",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,js}\" --fix",
    "demo:basic": "tsnd examples/express-basic",
    "demo:express": "tsnd examples/express",
    "demo:gcs": "tsnd  examples/express-gcs",
    "demo:gcs:direct": "tsnd examples/gcs-direct",
    "demo:node": "tsc && node examples/node-http-server",
    "demo:s3": "tsc && node examples/express-s3",
    "demo:tus": "tsnd examples/express-tus",
    "demo:fastify": "tsnd examples/fastify"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/chai": "^4.2.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/multiparty": "0.0.32",
    "@types/node": "~10.17.9",
    "@types/node-fetch": "^2.5.4",
    "@types/rimraf": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "express": "^4.17.1",
    "fastify": "^2.11.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.2",
    "node-mocks-http": "^1.8.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.3"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,examples}/**/*.{ts,js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
