  "engines": {
    "node": ">=8.0.0",
    "npm": ">= 5.7.0"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "ts-node test/server.ts",
    "dev": "nodemon",
    "storereset": "ts-node test/storage-reset.ts",
    "clean": "rimraf dist",
    "postbuild": "ts-node scripts/post-build.ts",
    "build": "npm run tslint && npm run clean && npm run build-ts &&  npm run postbuild",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "build-ts": "tsc",
