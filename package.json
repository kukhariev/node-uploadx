{
  "name": "node-uploadx",
  "version": "1.2.4",
  "description": "Node.js middleware for handling resumable uploads",
  "keywords": [
    "upload",
    "chunk",
    "express",
    "middleware"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
