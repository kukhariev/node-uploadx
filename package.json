{
  "name": "node-uploadx",
  "version": "3.3.0",
  "description": "Node.js resumable upload middleware",
  "keywords": [
    "upload",
    "chunk",
    "resumable",
    "express",
    "fastify",
    "middleware"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/node-uploadx.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "configstore": "^5.0.0",
    "debug": "^4.1.1",
    "multiparty": "^4.2.1"
  },
  "scripts": {
    "start": "tsnd test/server.ts",
    "dev": "tsnd --respawn --transpileOnly test/server.ts --log",
    "prettier": "prettier --write \"**/*.{json,md}\"",
    "build": "rimraf dist && tsc",
    "postbuild": "tsnd scripts/post-build.ts",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,js}\" --fix",
    "demo:node": "npm run build && node examples/node/uploadx",
    "demo:express": "tsnd examples/express",
    "demo:tus": "tsnd examples/node/tus",
    "demo:fastify": "tsnd examples/fastify"
  },
  "devDependencies": {
    "@types/bytes": "^3.0.0",
    "@types/chai": "^4.2.3",
    "@types/configstore": "^4.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/multiparty": "0.0.32",
    "@types/node": "^12.11.1",
    "@types/rimraf": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "2.3.3",
    "@typescript-eslint/parser": "2.3.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "express": "^4.17.1",
    "fastify": "^2.10.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.2",
    "node-mocks-http": "^1.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.6.4"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,examples}/**/*.{ts,js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
