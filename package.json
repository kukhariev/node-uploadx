{
  "name": "node-uploadx-src",
  "version": "6.0.0",
  "description": "Node.js resumable upload middleware",
  "keywords": [
    "resumable",
    "upload",
    "express",
    "uploadx",
    "middleware",
    "s3",
    "gcloud"
  ],
  "author": {
    "name": "Oleg Kukhariev",
    "url": "https://github.com/kukhariev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/node-uploadx.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.18.20",
    "npm": "use yarn instead",
    "yarn": ">=1.22.5"
  },
  "scripts": {
    "start": "yarn build & yarn --cwd examples run server",
    "build": "tsc --build packages",
    "build:watch": "tsc --build --watch packages",
    "clean": "tsc --build packages --clean && rimraf packages/*/lib upload files dist",
    "lint": "eslint . --fix --ext .js,.ts",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{json,md}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "29.1.2",
    "@types/node": "14.18.31",
    "@types/rimraf": "3.0.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "4.18.2",
    "husky": "8.0.1",
    "jest": "29.2.0",
    "lint-staged": "13.0.3",
    "memfs": "^3.4.7",
    "node-mocks-http": "1.11.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "supertest": "6.3.0",
    "ts-jest": "29.0.3",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "examples"
  ],
  "lint-staged": {
    "*.{md,json}": "prettier --write",
    "{packages,examples,test}/**/*.ts": "eslint --fix"
  }
}
